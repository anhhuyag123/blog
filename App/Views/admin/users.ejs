<!DOCTYPE html>
<html lang="en">

<head>
    <%- include layout/head %>
</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <!-- HEADER MOBILE-->
        <%- include layout/header_mobile %>
        <!-- END HEADER MOBILE-->
        <!-- END HEADER MOBILE-->

        <!-- MENU SIDEBAR-->
        <%- include layout/siderbar_user %>
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <%- include layout/header-desktop %>
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-lg-12">
                                <!-- USER DATA-->
                                <div class="user-data m-b-30">
                                    <h3 class="title-3 m-b-30">
                                        <i class="zmdi zmdi-account-calendar"></i>Users</h3>
                                    <div class="filters m-b-45">
                                        <div class="rs-select2--dark rs-select2--md m-r-10 rs-select2--border">
                                            <select class="js-select2" name="property">
                                                <option selected="selected">All Properties</option>
                                                <option value="">Products</option>
                                                <option value="">Services</option>
                                            </select>
                                            <div class="dropDownSelect2"></div>
                                        </div>
                                        <div class="rs-select2--dark rs-select2--sm rs-select2--border">
                                            <select class="js-select2 au-select-dark" name="time">
                                                <option selected="selected">All Time</option>
                                                <option value="">By Month</option>
                                                <option value="">By Day</option>
                                            </select>
                                            <div class="dropDownSelect2"></div>
                                        </div>
                                    </div>
                                    <div class="table-responsive table-data">
                                        <table class="table">
                                            <thead>
                                                <tr>
                                                    <td>
                                                        <label class="au-checkbox">
                                                            <input type="checkbox">
                                                            <span class="au-checkmark"></span>
                                                        </label>
                                                    </td>
                                                    <td>Name</td>
                                                    <td>First Name</td>
                                                    <td>Last Name</td>
                                                    <td>Role</td>
                                                    <td>Create Time</td>
                                                    <td>Update Time</td>
                                                    <td></td>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% if (data || data.users)
                                                    {
                                                        %>
                                                <%
                                                        for(var i =0 ;i< data.users.length;i++)
                                                        {
                                                        %>
                                                <tr>
                                                    <td>
                                                        <label class="au-checkbox">
                                                            <input type="checkbox">
                                                            <span class="au-checkmark"></span>
                                                        </label>
                                                    </td>
                                                    <td>
                                                        <%
                                                            var fullname = data.users[i].first_name + " " +  data.users[i].last_name
                                                            %>
                                                        <div class="table-data__info">
                                                            <h6>
                                                                <%= fullname %>
                                                            </h6>
                                                            <span>
                                                                <a href="#">
                                                                    <%= data.users[i].email%>
                                                                </a>
                                                            </span>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <p>
                                                            <%= data.users[i].first_name %>
                                                        </p>
                                                    </td>
                                                    <td>
                                                        <p>
                                                            <%= data.users[i].last_name %>
                                                        </p>
                                                    </td>
                                                    <td>
                                                        <span class="role user">user</span>
                                                    </td>
                                                    <td>
                                                        <p>
                                                            <%= data.users[i].create_at.getDate() %>/<%= data.users[i].create_at.getMonth() %>/<%= data.users[i].create_at.getFullYear() %>
                                                        </p>
                                                    </td>
                                                    <td>
                                                        <p>
                                                            <%= data.users[i].update_at.getDate() %>/<%= data.users[i].update_at.getMonth() %>/<%= data.users[i].update_at.getFullYear() %>
                                                        </p>
                                                    </td>
                                                    <td>
                                                        <span class="more">
                                                            <i class="zmdi zmdi-more"></i>
                                                        </span>
                                                    </td>
                                                </tr>
                                                <%
                                                        }
                                                        %>
                                                <%
                                                    }
                                                        %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <!-- END USER DATA-->
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="copyright">
                                    <p>Copyright Â© 2018 Colorlib. All rights reserved. Template by <a href="https://colorlib.com">Colorlib</a>.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <%- include layout/javascript %>
    </div>
</body>

</html>